    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.1.0",
    "@types/nodemailer": "^7.0.3",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "fastify-tsconfig": "^3.0.0",
    "prisma": "^6.18.0",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.2"
  }
